{
  "expo": {
    "name": "Wagara Blast",
    "slug": "wagara-blast",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "scheme": "wagara-blast",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#1a0a2e"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.wagara.blast",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#1a0a2e"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.wagara.blast",
      "permissions": [
        "android.permission.RECORD_AUDIO",
        "android.permission.MODIFY_AUDIO_SETTINGS"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      "expo-dev-client",
      "expo-audio",
      "expo-font",
      "react-native-reanimated"
    ],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "62faada5-0ada-4c2e-aeec-233791dea7f6"
      }
    }
  }
}
